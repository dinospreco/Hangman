<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AdminController</servlet-name>
  	<display-name>AdminController</display-name>
  	<description></description>
  	<servlet-class>controller.AdminController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SuperUserController</servlet-name>
  	<display-name>SuperUserController</display-name>
  	<description></description>
  	<servlet-class>controller.SuperUserController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AdminController</servlet-name>
  	<url-pattern>/AdminController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SuperUserController</servlet-name>
  	<url-pattern>/SuperUserController</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>view/home.jsp</welcome-file>
  </welcome-file-list>

</web-app>
